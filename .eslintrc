{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "valid-jsdoc": [
            "error",
            {
                "requireReturn": true,
                "requireReturnType": false,
                "requireParamDescription": false,
                "requireReturnDescription": false
            }
        ]
    }
}